{
  "name": "dme-portal-angular5",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:analyzer": "[ -d dist ] || mkdir dist && ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "start": "ng serve --live-reload-client http://localhost:4200 --disableHostCheck true",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json -s -n NikoHealth-Documentation -a src/assets/images/examples --hideGenerator --disableCoverage --disablePrivate --disableProtected --disableLifeCycleHooks --theme Laravel --includes src/app/shared --port 4201"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint 'src/**/*.ts'",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ng-idle/core": "^2.0.0-beta.15",
    "@ng-idle/keepalive": "^2.0.0-beta.15",
    "@types/lodash": "^4.14.118",
    "ag-grid": "^18.0.0",
    "ag-grid-angular": "^18.0.0",
    "angular-2-local-storage": "^2.0.0",
    "angular2-moment": "^1.8.0",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "ng-pick-datetime": "^5.2.6",
    "ng-pick-datetime-moment": "^1.0.6",
    "ng2-file-upload": "^1.3.0",
    "ngx-cookie-service": "^1.0.10",
    "ngx-mask": "^6.0.1",
    "ngx-perfect-scrollbar": "^5.3.5",
    "ngx-showdown": "^4.0.2",
    "ngx-toastr": "~8.10.2",
    "ngx-webstorage": "^2.0.1",
    "normalize-scss": "^7.0.1",
    "rxjs": "^5.5.0",
    "showdown": "^1.8.6",
    "signature_pad": "^3.0.0-beta.3",
    "tabex": "^1.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.3.2",
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/language-service": "^5.2.11",
    "@compodoc/compodoc": "^1.1.6",
    "@ngtools/webpack": "1.10.2",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.113",
    "autoprefixer": "^7.2.3",
    "circular-dependency-plugin": "^4.2.1",
    "codelyzer": "^4.4.2",
    "copy-webpack-plugin": "~4.4.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^3.0.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^9.2.1",
    "lodash-es": "^4.17.11",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.1.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rxjs-tslint": "0.1.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylus-loader": "^3.0.1",
    "tippy.js": "^2.6.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
